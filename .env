# .env

# Path to the Google Cloud service account credentials JSON file
GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/service-account-key.json

# Your Google Cloud Project ID
PROJECT_ID=your-google-cloud-project-id

# Region or location for Vertex AI (e.g., us-central1)
LOCATION=us-central1

# Embedding model name (you can change it to the specific model you're using)
EMBEDDING_MODEL_NAME=textembedding-gecko@001

# Name for your Matching Engine index
INDEX_DISPLAY_NAME=my_rag_index

# Name for the Matching Engine endpoint
ENDPOINT_DISPLAY_NAME=my_rag_endpoint

# Deployed index ID for querying
DEPLOYED_INDEX_ID=my_deployed_index

# Name of the Google Cloud Storage bucket (if required, for saving intermediate data)
BUCKET_NAME=your-google-cloud-storage-bucket-name